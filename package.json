{
  "name": "connect-ts-api",
  "version": "4.6.2",
  "main": "lib/connect.js",
  "typings": "lib/connect",
  "scripts": {
    "build": "tsc",
    "lint": "tslint **/*.ts",
    "test": "jest",
    "test-alone": "nyc ava",
    "jest-codemods": "jest-codemods"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "files": [
    "lib/"
  ],
  "description": "Connect API in TypeScript",
  "keywords": [
    "spotware",
    "connect",
    "ctratder"
  ],
  "author": "Spotware Services <connect@spotware.com>",
  "private": false,
  "dependencies": {
    "hat": "0.0.3"
  },
  "devDependencies": {
    "@types/jest": "20.0.5",
    "@types/node": "^6.0.58",
    "connection-adapter": "spotware/connection-adapter#v1.8.0",
    "husky": "0.13.1",
    "jest": "20.0.4",
    "nyc": "10.1.2",
    "rxjs": "5.3.0",
    "ts-jest": "20.0.7",
    "tslint": "3.15.1",
    "typescript": "2.3.0"
  }
}
